page {
  width: 100%;
	height:auto;
	background:#F5F5F5;
}
.shopRecom {
  width: 100%;
  padding-bottom: 100rpx;
}
.shopRecomTit {
  width: 100%;
  padding-bottom: 36rpx;
  overflow: hidden;
  background: #FFFFFF;
}
.shopRecomTit .select {
  width: 670rpx;
  margin: 0 auto;
  overflow: hidden;
  margin-top: 36rpx;
}
.shopRecomTit .select .selected {
  position: relative;
  width: 300rpx;
}
.select .selected .picker text {
  display: inline-block;
  font-size: 24rpx;
  color:rgba(255,64,129,1);
  font-family: "SourceHanSansSC-regular";
}
.select .selected .picker text:last-child {
  margin-left: 30rpx;
  color: #A1A1A1;
}
.select .selected .absolut {
  position: absolute;
  top: 16rpx;
  right: 12rpx;
  width: 30rpx;
  height: 30rpx;
  overflow: hidden;
}
.select .selected .absolut image {
  width: 24rpx;
  height: 24rpx;
  vertical-align: top;
  margin-top: 4rpx;
}
.count {
  width: 706rpx;
  padding-top: 34rpx;
  padding-bottom: 24rpx;
  margin: 0 auto;
  margin-top: 20rpx;
  border-radius: 20rpx;
  background: #FFFFFF;
}
.countIn {
  position: relative;
  width: 674rpx;
  margin: 0 auto;
}
.countIn .shopName {
  width: 100%;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 28rpx;
  color: #222222;
  font-family: "SourceHanSansSC-regular";
}
.countIn .Radioabsolut {
  position: absolute;
  top: -4rpx;
  right: 20rpx;
  width: 32rpx;
  height: 32rpx;
  border-radius: 50%;
  border: 2rpx solid #DDDDDD;
}
.countIn .selectedAbs {
  width: 32rpx;
  height: 32rpx;
  background:#EF738E;
  border: 2rpx solid #EF738E;
}
.countIn .spot {
  position: absolute;
  top: 0rpx;
  left: 0rpx;
  right: 0rpx;
  bottom: 0rpx;
  margin: auto;
  width: 20rpx;
  height: 20rpx;
  border-radius: 50%;
  background:#FFFFFF;
  border: 2rpx solid #EF738E;
}
.countIn .adress {
  width: 100%;
  height: 36rpx;
  line-height: 36rpx;
  margin-top: 6rpx;
  font-size: 24rpx;
  color: #A1A1A1;
  font-family: "SourceHanSansSC-regular";
}
.countIn .money {
  width: 100%;
  height: 30rpx;
  margin-top: 14rpx;
  font-size: 24rpx;
  color: #A1A1A1;
  font-family: "SourceHanSansSC-regular";
}
.countIn .money text:first-child {
  color: #FF5105;
}
.countIn .money text:last-child {
  font-size: 36rpx;
  margin: 0 12rpx 0 6rpx;
  color: #FF5105;
}
.countIn .imgBox {
  width: 100%;
  margin-top: 26rpx;
}
.countIn .imgBox image {
  width: 206rpx;
  height: 174rpx;
  margin-right: 29rpx;
  border-radius: 16rpx;
}
.countIn .imgBox .active {
  margin-right: 0rpx;
}
.shopRecomHeader{
  position: relative;
  height: 590rpx;
  background: #fff;
  margin-bottom: 20rpx;
}
.shopRecomHeader .headerBg{
  width: 100%;
  height: 180rpx;
}
.shopRecomHeader .headerBg image{
  height: 100%;
  width: 100%;
}
.formData{
  box-sizing: border-box;
  left: 30rpx;
  top: 100rpx;
  position: absolute;
  width: 690rpx;
  height: 456rpx;
  background: #fff;
  box-shadow:0px 2px 6px 0px rgba(0, 0, 0, 0.18);
  border-radius:28rpx;
  padding: 30rpx 20rpx;
}
.formData>view{
  border-bottom: 2px solid #E5E5E5;
  height:120rpx;
  margin-bottom: 20rpx;
}
.formData .leftImg{
  width: 100rpx;
  height: 100rpx;
  float: left;
  background: #ffffff;
  border: 1px dashed #eeeeee;
}
.formData .leftImg image{
  width: 100%;
  height: 100%;
}
.formData .rightForm{
  float: left;
  width: 540rpx;
  box-sizing: border-box;
  padding-left: 40rpx;
}
.formData .rightForm .title{
  margin-top: 16rpx;
  height:32rpx;
  line-height:32rpx;
  font-size:32rpx;
  font-family:SourceHanSansCN-Regular;
  font-weight:400;
  color:rgba(255,64,129,1);
}
.rightForm .tui-picker-detail {
  width: 100%;
  height:32rpx;
  line-height: 32rpx;
  margin-top: 18rpx;
  font-size:26rpx;
  font-family:SourceHanSansCN-Regular;
  color: #BDBDBD;
}

.radio-group view {
  height: 40rpx;
  line-height: 40rpx;
  margin-top: 12rpx;
}

.radio-group view text {
  display: inline-block;
  vertical-align: middle;
  font-size: 24rpx;
  color: #796f73;
  margin-left: 10rpx;
  font-family: "SourceHanSansSC-regular";
}

.radio-group view radio {
  width: 38rpx;
  height: 38rpx;
  border: none;
}

.radio-group view label:first-child{
  width:170rpx;
  display:inline-block;
}
.radio-group view label:last-child{
  width:280rpx;
  display:inline-block;
}

.radio-group view radio .wx-radio-input {
  border-radius: 50%;
  width: 24rpx;
  height: 24rpx;
  line-height: 24rpx;
}

radio .wx-radio-input.wx-radio-input-checked::before {
  border-radius: 50%;
  width: 40rpx;
  height: 40rpx;
  line-height: 40rpx;
  margin-top: 0rpx;
  text-align: center;
  font-size: 25rpx;
  color: #fff;
  background: #ef738e;
  border-color: #ef738e;
}
.borderBottomNull{
  border-bottom: 0 !important;
}
.submit {
  width: 690rpx;
  height: 84rpx;
  line-height: 84rpx;
  text-align: center;
  font-size: 30rpx;
  color: #FFFFFF;
  font-family: "SourceHanSansCN-Regular";
  background: #FF4081;
  border-radius: 10rpx;
  margin: 0 auto;
  margin-top: 86rpx;
}