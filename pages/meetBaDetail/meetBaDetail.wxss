}
.plcount .plMessage {
  width: 100%;
  line-height: 40rpx;
  text-align: justify;
  margin-top: 12rpx;
  font-size: 28rpx;
  color: #222222;
  font-family: "SourceHanSansSC-regular";
}
.plcount .plTime {
  width: 100%;
  height: 36rpx;
  line-height: 36rpx;
  margin-top: 28rpx;
  font-size: 22rpx;
  color: #BDBDBE;
  font-family: "SourceHanSansSC-regular";
}
.sendPl {
  position: fixed;
  left: 0rpx;
  bottom: 0rpx;
  width: 100%;
  height: 100rpx;
  background: #F5F5F5;
}
.sendPl input {
  width: 660rpx;
  height: 70rpx;
  line-height: 70rpx;
  margin: 0 auto;
  margin-top: 15rpx;
  padding-left: 30rpx;
  background: #FFFFFF;
  border-radius: 60rpx;
  font-size: 28rpx;
  color: #101010;
  font-family: "SourceHanSansSC-regular";
}
.sendPl .placeholder {
  color: #DFDFDF;
}
.sendPl image {
  position: absolute;
  top: 26rpx;
  right: 70rpx;
  width: 48rpx;
  height: 48rpx;
}