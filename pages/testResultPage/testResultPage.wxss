/* pages/testResultPage/testResultPage.wxss */
.resultMain{
    margin-top: 56rpx;
    text-align: center;
}
.resultMain .type{
    height: 36rpx;
    line-height: 36rpx;
    color: rgba(161, 161, 161, 1);
    font-size: 24rpx;
    font-family: SourceHanSansSC-regular;
}
.resultMain .result{
    margin-top: 8rpx;
    height: 56rpx;
    line-height: 56rpx;
    color: rgba(34, 34, 34, 1);
    font-size: 40rpx;
    font-family: PingFangSC-bold;
}
.imageMain{
    margin:100rpx auto 100rpx;
    width:486rpx;
    height:340rpx;
}
.imageMain image{
    width: 100%;
    height: 100%;
}
.explain{
    margin: 0 40rpx;
    color: rgba(25, 25, 25, 1);
    font-size: 26rpx;
    text-align: justify;
    font-family: SourceHanSansSC-regular;
}
.explain text{
    font-weight: bold;
}
.intoApp{
    margin: 136rpx auto 126rpx;
    width: 274rpx;
    height: 88rpx;
    line-height: 88rpx;
    border-radius: 60rpx;
    background-color: rgba(239, 115, 142, 1);
    text-align: center;
    box-shadow: 2rpx 2rpx 46rpx 0rpx rgba(255, 199, 199, 1);
    border: 2rpx solid rgba(255, 255, 255, 0);
    color: rgba(255, 255, 255, 1);
    font-size: 32rpx;
    font-family: SourceHanSansSC-regular;
}