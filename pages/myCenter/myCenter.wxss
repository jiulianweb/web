page {
  width: 100%;
	height:auto;
	background:#FFFFFF;
}
.myCenter {
  width: 100%;
  padding-bottom: 100rpx;
}
.userName {
  position: relative;
  width: 100%;
  height: 300rpx;
  line-height: 300rpx;
  background: #EF738E;
}
.userName text {
  display: inline-block;
  margin-left: 28rpx;
  font-size: 56rpx;
  color: #FFFFFF;
  font-family: "SourceHanSansSC-regular";
}
.userName image {
  position: absolute;
  top: 0rpx;
  bottom: 0rpx;
  right: 100rpx;
  margin: auto;
  width: 180rpx;
  height: 180rpx;
  border-radius: 50%;
}
.tabNav {
  width: 690rpx;
  margin: 0 auto;
  margin-top: 20rpx;
  overflow: hidden;
}
.barLink {
  width: 25%;
  margin-top: 30rpx;
}
.barLink view {
  width: 100%;
  text-align: center;
}
.barLink image {
  width: 60rpx;
  height: 60rpx;
}
.barLink .text {
  font-size: 28rpx;
  color: #222222;
  font-family: "SourceHanSansSC-regular";
}
.swiper {
  height: 400rpx;
  margin-top: 46rpx;
}
.swiper image {
  width: 100%;
}
.marginAuto {
  width: 690rpx;
  margin: 0 auto;
  margin-top: 30rpx;
}
.marginAuto .title {
  width: 100%;
  height: 42rpx;
  line-height: 42rpx;
  margin-top: 10rpx;
  margin-bottom: 10rpx;
  font-size: 30rpx;
  color: #101010;
  font-family: "SourceHanSansSC-regular";
}
.marginAuto .count {
  margin-left: 20rpx;
  font-size: 28rpx;
  color: #8E8E8E;
  font-family: "SourceHanSansSC-regular";
}