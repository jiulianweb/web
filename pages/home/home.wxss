page, .content {
  width: 100%;
}

.content {
  width: 100%;
  overflow: hidden;
  padding-bottom: 80rpx;
}

.title {
  padding-top: 22rpx;
  padding-left: 42rpx;
  height: 56rpx;
  line-height: 56rpx;
  color: rgba(34, 34, 34, 1);
  font-size: 40rpx;
  text-align: left;
  font-family: PingFangSC-regular;
}

.form {
  margin-top: 14rpx;
}

.form .section {
  position: relative;
  margin-left: 46rpx;
  margin-right: 40rpx;
  border-bottom: 1px solid #f1f1f1;
}

.form .section .defaultClass {
  color: rgba(189, 189, 190, 1);
}

.form .section input {
  height: 100rpx;
  line-height: 100rpx;
  color: #333;
  font-size: 28rpx;
  text-align: left;
  font-family: PingFangSC-regular;
}

.form .section image {
  position: absolute;
  right: 0;
  top: 44rpx;
  height: 32rpx;
  width: 32rpx;
}

.form .section picker {
  height: 50px;
  line-height: 50px;
  color: rgba(189, 189, 190, 1);
  font-size: 28rpx;
}

.form .section picker .realDate {
  color: #333;
}

.form .vCode  view {
  position: absolute;
  top: 22rpx;
  right: 0;
  width: 178rpx;
  height: 56rpx;
  line-height: 56rpx;
  border-radius: 60rpx;
  background-color: rgba(255, 255, 255, 0);
  font-size: 26rpx;
  text-align: center;
  box-shadow: 0px 0px 0px 0px rgba(255, 255, 255, 0);
  border: 2rpx solid rgba(239, 115, 142, 1);
  color: rgba(239, 115, 142, 1);
  font-family: PingFangSC-regular;
  z-index: 1;
}

.chooseSex {
  overflow: hidden;
  margin: 68rpx auto 0;
  width: 480rpx;
}

.chooseSex .head_icon {
  float: left;
  width: 170rpx;
  padding-bottom: 10rpx;
  text-align: center;
}

.chooseSex .head_icon:last-child {
  float: right;
}

.chooseSex .head_icon image {
  height: 170rpx;
  width: 170rpx;
  vertical-align: middle;
}

.chooseSex .head_icon text {
  vertical-align: middle;
  color: rgba(189, 189, 190, 1);
  font-size: 28rpx;
  font-family: PingFangSC-regular;
  margin-left: 18rpx;
  margin-top: 0rpx;
}

.form .attention {
  color: rgba(189, 189, 190, 1);
  font-size: 24rpx;
  text-align: center;
  font-family: PingFangSC-regular;
}

.operate {
  margin-top: 48rpx;
}

.operate .button {
  width: 670rpx;
  height: 88rpx;
  line-height: 88rpx;
  margin: 0 auto;
  border-radius: 60rpx;
  background-color: rgba(239, 115, 142, 1);
  text-align: center;
  border: 2rpx solid rgba(255, 255, 255, 0);
  color: rgba(255, 255, 255, 1);
  font-size: 32rpx;
  font-family: PingFangSC-regular;
}

.dialog {
  height: 115%;
  width: 100%;
  position: absolute;
  top: 0;
  left: 0;
  background: #333;
  opacity: 0.6;
  z-index: 10;
}

.privacyPolicy {
  width: 584rpx;
  height: 806rpx;
  background: #fff;
  position: fixed;
  left: 0rpx;
  top: 0rpx;
  right: 0rpx;
  bottom: 0rpx;
  margin: auto;
  z-index: 11;
  line-height: 20px;
  border-radius: 10px;
  border: 1px solid rgba(255, 255, 255, 0);
}

.privacyPolicy .title {
  margin: 42rpx 0 34rpx 0;
  text-align: center;
  padding: 0;
  height: 48rpx;
  line-height: 48rpx;
  color: rgba(16, 16, 16, 1);
  font-size: 32rpx;
  font-family: SourceHanSansSC-regular;
}

.privacyPolicy .detail {
  line-height: 40rpx;
  padding: 0 50rpx 0 58rpx;
  color: rgba(16, 16, 16, 1);
  font-size: 28rpx;
  text-align: justify;
  font-family: SourceHanSansSC-regular;
}

.privacyPolicy .operateMain {
  width: 100%;
  overflow: hidden;
  position: absolute;
  bottom: 0;
  border-top: 2rpx solid #f1f1f1;
  box-sizing: border-box;
  height: 100rpx;
  font-size: 32rpx;
  line-height: 100rpx;
  color: rgba(189, 189, 190, 1);
  font-family: SourceHanSansSC-regular;
  text-align: center;
}

.privacyPolicy .operateMain .agree {
  float: left;
  width: 292rpx;
  border-left: 2rpx solid #f1f1f1;
  box-sizing: border-box;
  color: #ef738e;
}

.privacyPolicy .operateMain .disagree {
  float: left;
  width: 292rpx;
}
