page {
  width: 100%;
	height:auto;
	background:#F5F5F5;
}
.userInfo {
  width: 100%;
}
.userInfoIn {
  width: 100%;
  overflow: hidden;
  background: #FFFFFF;
}
.marginAuto {
  width: 690rpx;
  margin: 0 auto;
  margin-top: 30rpx;
  padding-bottom: 80rpx;
}
.uploadImg {
  width: 100%;
}
.uploadImg image {
  width: 208rpx;
  height: 208rpx;
  margin-right: 33rpx;
}
.uploadImg image:last-child {
  margin-right: 0rpx;
}
.marginAuto .tips {
  width: 100%;
  height: 40rpx;
  line-height: 40rpx;
  margin-top: 10rpx;
  font-size: 28rpx;
  color: #D5D5D5;
  font-family: "SourceHanSansSC-regular";
}
.marginAuto .baseInfo {
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  margin-top: 10rpx;
  font-size: 40rpx;
  color: #222222;
  font-family: "SourceHanSansSC-regular";
}
.marginAuto .infoBox {
  position: relative;
  width: 100%;
  height: 90rpx;
  border-bottom: 2rpx solid #F5F5F5;
  overflow: hidden;
}
.marginAuto .infoBox text {
  display: inline-block;
  height: 90rpx;
  line-height: 90rpx;
  font-size: 28rpx;
  color: #101010;
  font-family: "SourceHanSansSC-regular";
}
.marginAuto .infoBox input {
  float: right;
  width: 300rpx;
  height: 90rpx;
  text-align: right;
  line-height: 90rpx;
  vertical-align: middle;
  font-size: 28rpx;
  color: #8E8E8E;
  font-family: "SourceHanSansSC-regular";
}
.marginAuto .infoBox .school {
  float: right;
  height: 90rpx;
  text-align: right;
  line-height: 90rpx;
  vertical-align: middle;
  font-size: 28rpx;
  color: #8E8E8E;
  font-family: "SourceHanSansSC-regular";
}
.marginAuto .infoBox .select_icon {
  float: right;
  right: 0rpx;
  width: 30rpx;
  height: 30rpx;
  margin-left: 14rpx;
  margin-top: 33rpx;
}
.marginAuto .otherInfo {
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  margin-top: 20rpx;
  font-size: 32rpx;
  color: #222222;
  font-family: "SourceHanSansSC-regular";
}
.marginAuto .otherInput {
  width: 100%;
  height: 80rpx;
  margin-top: 10rpx;
}
.marginAuto .otherInput input {
  width: 680rpx;
  height: 76rpx;
  padding-left: 10rpx;
  border: 2rpx solid #E5E5E5;
  border-radius: 6rpx;
  font-size: 28rpx;
  color: #8E8E8E;
  font-family: "SourceHanSansSC-regular";
}
.marginAuto .sbumit {
  width: 100%;
  height: 88rpx;
  line-height: 88rpx;
  text-align: center;
  background: #EF738E;
  font-size: 32rpx;
  color: #FFFFFF;
  font-family: "SourceHanSansSC-regular";
  border-radius: 60rpx;
  margin-top: 116rpx;
}